    "web": "cd WhatToDoAI/mobile && expo start --web",
    "web:app": "cd WhatToDoAI/web && npm start"
  },
  "private": true,
  "workspaces": [
    "WhatToDoAI/mobile",
    "WhatToDoAI/web"
  ],
  "dependencies": {
    "expo-dev-client": "^5.0.20"
  }
}
